{
  "name": "zipscene-api-client",
  "version": "2.0.1",
  "description": "Zipscene API Client",
  "main": "./dist/lib/jsonrpc-api-client.js",
  "keywords": [
    "zipscene"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zipscene/zipscene-api-client.git"
  },
  "scripts": {
    "lint": "node ./node_modules/.bin/eslint . || exit 0",
    "docs": "node ./node_modules/.bin/yuidoc .",
    "test": "npm run-script build && node ./node_modules/.bin/mocha ./dist/test -R spec",
    "build": "rm -rf dist && ./node_modules/.bin/broccoli build dist && rm -rf tmp",
    "prepublish": "npm run-script build && npm run-script docs"
  },
  "dependencies": {
    "babel-runtime": "^6.1.18",
    "decamelize": "^1.1.1",
    "es6-promise": "^3.0.2",
    "lodash": "^3.10.1",
    "pasync": "^1.4.0",
    "request": "^2.67.0",
    "request-promise": "^1.0.2",
    "xerror": "^1.1.0",
    "zstreams": "^3.2.2"
  },
  "devDependencies": {
    "broccoli": "^0.16.8",
    "broccoli-babel-transpiler": "^5.5.0",
    "broccoli-cli": "^1.0.0",
    "broccoli-merge-trees": "^1.0.0",
    "broccoli-static-compiler": "^0.2.2",
    "broccoli-wrap": "0.0.2",
    "chai": "^3.4.1",
    "eslint": "^1.9.0",
    "mocha": "^2.3.3",
    "mongodb-promise": "0.0.4",
    "sinon": "^1.17.2",
    "source-map-support": "^0.3.3",
    "uuid": "^2.0.1",
    "yuidocjs": "^0.9.0"
  },
  "license": "Apache-2.0"
}